#---------------------------------------------------------------------------------
#NOTICE: these are methods, that means they are called with -x, e.g. gracetree.rb -y 8 -m 5 -x jtkaakdate
#        always use double quotes to keep an argument from being split (single quotes are not supported)
#---------------------------------------------------------------------------------
methodlist:

  jtkgpsdoy:   -t jtkgps     -x awk -g "{print $2}"
  jtkgpsdate:  -t jtkgps     -x awk -g "{print $3}"
  jtkgpsmjd:   -t jtkgps     -x awk -g "{print $4}"
  jtkgpsOOID:  -t jtkgps     -x awk -g "{print $5}" #same as JOBID

  jtkaakdoy:   -t jtkaak     -x awk -g "{print $2}"
  jtkaakdate:  -t jtkaak     -x awk -g "{print $3}"
  jtkaakmjd:   -t jtkaak     -x awk -g "{print $4}"
  jtkaakOOID:  -t jtkaak     -x awk -g "{print $5}"
  jtkaakEDID:  -t jtkaak     -x awk -g "{print $6}" #same as JOBID

  jtkaccdoy:   -t jtkacc     -x awk -g "{print $2}"
  jtkaccdate:  -t jtkacc     -x awk -g "{print $3}"
  jtkaccmjd:   -t jtkacc     -x awk -g "{print $4}"
  jtkaccOOID:  -t jtkacc     -x awk -g "{print $5} {nextfile}" #some jtkacc files have duplicate lines
  jtkaccACID:  -t jtkacc     -x awk -g "{print $6} {nextfile}" #same as JOBID

  jtkgrvdoy:      -t jtkgrv  -x awk -g "{print $2}"
  jtkgrvdate:     -t jtkgrv  -x awk -g "{print $3}"
  jtkgrvmjd:      -t jtkgrv  -x awk -g "{print $4}"
  jtkgrvACID:     -t jtkgrv  -x awk -g "{print $5}"
  jtkgrvGRID:     -t jtkgrv  -x awk -g "{print $6}" #same as JOBID
  jtkgrvstart:    -t jtkgrv  -x awk -g "{print $12}"
  jtkgrvduration: -t jtkgrv  -x awk -g "{print $13}"

  jtkdoy:      -t jtk        -x awk -g "{print $2}"
  jtkdate:     -t jtk        -x awk -g "{print $3}"
  jtkmjd:      -t jtk        -x awk -g "{print $4}"
  jtkACID:     -t jtk        -x awk -g "{print $5}"
  jtkGRID:     -t jtk        -x awk -g "{print $6}" #same as JOBID (reported in the gps/krrreg)
  jtkstart:    -t jtk        -x awk -g "{print $12}"
  jtkduration: -t jtk        -x awk -g "{print $13}"

  #NOTICE: this shows these IDs for the whole month, irrespective of -d
  krrregGRID:  -t krrreg -x awk -g "BEGIN {FS='.'}; {print $2}"
  krrregdate:  -t krrreg -x awk -g "BEGIN {FS='[_-.]'}; {print sprintf('20%02d-%02d-%02d',$11,$12,$13)}"
  krrregargs:  -t krrreg -x awk -g "BEGIN {FS='[_-.]'}; {print sprintf('-D RL05_%02d-%02d -d %02d',$11,$12,$13)}"
  krrobsEDID:  -t krrobs -x awk -g "BEGIN {FS='.'}; {print $3}"
  krrobsdate:  -t krrobs -x awk -g "BEGIN {FS='[._]'}; {print $4}"
  krrobsyear:  -t krrobs -x awk -g "BEGIN {FS='[.\-]'}; {print $2}"
  krrobsargs:  -t krrobs -x awk -g "BEGIN {FS='[_-.]'};
                                        /corral-tacc\/utexas\/csr/ {print '-y',$13-2000,'-m',$14,'-d',$15;};
                                        /corral\/csr\/grace/       {print '-y',$11-2000,'-m',$12,'-d',$13};"

  #NOTICE: most if not all all of this stuff only works on RL05
  RL05date:    -t pts -x year+month+day
  job_aesopJAID:  -t job_aesop -x jobid
  arclist:     -t pts        -x arc
  arcdate:                   -x jtkdate+jtkmjd+arclist
  arctimeline:               -x arclist+jtkdate+jtkmjd+jtkstart+jtkduration
  dateconsistency:           -x inputdate+aakdate+accattdate+jtkdate
  ptsarcstart: -t pts -x awk -g "{print $7}"
  ptsparam:    -t pts -x awk -g "{print $1, $2}"
  ptsyear:     -t pts -x year
  ptsmonth:    -t pts -x month
  ptsday:      -t pts -x day
  ptsdate:            -x ptsyear+ptsmonth+ptsday
